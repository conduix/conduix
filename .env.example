# Conduix Environment Variables
# Copy this file to .env and fill in your values
# cp .env.example .env

# Database
DB_HOST=localhost
DB_PORT=3307
DB_USER=conduixuser
DB_PASSWORD=conduixpassword
DB_NAME=conduix

# Redis
REDIS_ADDR=localhost:6379
REDIS_PASSWORD=

# JWT
JWT_SECRET=your-secret-key-change-in-production

# GitHub OAuth2
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_REDIRECT_URL=http://localhost:8080/api/v1/auth/callback

# Google OAuth2 (optional)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URL=http://localhost:8080/api/v1/auth/callback

# Users Config
USERS_CONFIG_PATH=../config/users.yaml

# Frontend URL (for OAuth callback redirect)
FRONTEND_URL=http://localhost:3000

# DataType Max Depth (default: 10)
MAX_DATATYPE_DEPTH=10
